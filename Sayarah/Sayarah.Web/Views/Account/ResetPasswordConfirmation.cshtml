@{
    ViewBag.Title = "ResetPasswordConfirmation";
}

<!--begin::Reset Password Confirmation-->
<div class="login-form">
    <!--begin::Title-->
    <div class="pb-5">
        <h3 class="font-weight-bolder text-dark font-size-h2 font-size-h1-lg">@L("Pages.ResetPassword.ResetPassword")</h3>

        <p class="text-muted font-weight-bold font-size-h4">
            @L("Pages.ResetPassword.SuccessfullyReset")
        </p>
    </div>
    <!--end::Title-->
    <!--begin::Form group-->
    <div class="form-group d-flex flex-wrap">

        <a href="@Url.Action("Login", "Account")" class="btn btn-theme font-weight-bolder font-size-h6 px-8 py-4 my-3">@L("Pages.Login.Login")</a>
    </div>
    <!--end::Form group-->


</div>
<!--end::Reset Password Confirmation-->
